{
  "name": "babel-preset-react-styles",
  "version": "0.0.0",
  "description": "Styles in react without thinking about it",
  "private": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.2",
    "babel-plugin-emotion": "^10.0.9",
    "babel-plugin-split-styles": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@types/jest": "^24.0.13",
    "babel-plugin-tester": "^6.2.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "scripts": {
    "build": "npm run build:lib & npm run build:es",
    "build:es": "tsc --outDir ../node_modules/babel-preset-react-styles/es --module es2015",
    "build:lib": "tsc --outDir ../node_modules/babel-preset-react-styles/lib --module commonjs",
    "clean": "rimraf es lib coverage ../node_modules/babel-preset-react-styles",
    "prebuild": "npm run clean",
    "prepare": "rimraf es lib && cp -r ../node_modules/babel-preset-react-styles/es es && cp -r ../node_modules/babel-preset-react-styles/lib lib",
    "postbuild": "rimraf {lib,es}/**/__tests__ && cp package.json ../node_modules/babel-preset-react-styles",
    "posttest": "npm run typecheck",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --updateSnapshot --coverage false",
    "typecheck": "tsc --noEmit"
  },
  "author": "Fabrice Weinberg <Fabrice@weinberg.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/FWeinb/babel-plugin-split-styles.git"
  },
  "license": "MIT"
}
